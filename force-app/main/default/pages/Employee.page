<apex:page standardController="Employee__c">
    <apex:form >
    <apex:pageBlock title="Employee Edit">
     <apex:pageBlockSection title="Information About Employee">
         <apex:inputField value="{!Employee__c.First_name__c}"/>
         <apex:inputField value="{!Employee__c.Last_name__c}"/>
         <apex:inputField value="{!Employee__c.Email__c}"/>
         <apex:inputField value="{!Employee__c.Address__c}"/>
         <apex:inputField value="{!Employee__c.Country__c}"/>
         <apex:inputField value="{!Employee__c.State__c}"/>
         <apex:inputField value="{!Employee__c.Social_Page__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton value="save" action="{!Save}"/>
        </apex:pageBlockButtons>
    </apex:pageBlock>
    </apex:form>
</apex:page>